{
    "name": "artisanry/invoice",
    "description": "Easy Invoices for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel Invoice",
        "Laravel-Invoice"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0",
        "dompdf/dompdf": "^0.8",
        "moneyphp/money": "^3.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "5.3.1",
        "mockery/mockery": "1.2.4",
        "phpunit/phpunit": "8.4.0"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Invoice\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Invoice\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Invoice\\InvoiceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
